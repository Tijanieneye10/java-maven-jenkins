pipeline {
    agent any
    stages {
        stage("build") {
            steps {
                echo "Here is my first build"
                echo "Am still learning"
            }
        }

        stage("test") {
            steps {
                echo "test running..."
                echo "test passed..."
            }
        }

        stage("deploy") {
            steps {
                echo "deploying..."
                echo "deployed"
            }
        }
    }

}
